<div id="{{this.id}}" class="row">
  <div class="col-xs-1 col-mid-1">
    <i class="fa fa-times"/>
  </div>
  <div class="col-xs-3 col-mid-3">{{this.beer}}</div>
  <div class="col-xs-3 col-mid-3">{{this.brewery}}</div>
  <div class="col-xs-3 col-mid-3">{{this.style}}</div>
  <div class="col-xs-1 col-mid-1">{{this.season}}</div>
  <div class="col-xs-1 col-mid-1">  
    {{#if this.isGood}}
      <i class="fa fa-thumbs-up" />
    {{else}}
      <i class="fa fa-thumbs-down" />
    {{/if}}
  </div>
</div>